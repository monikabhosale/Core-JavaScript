// Nested while loop
<html>
<head><title> monika bhosale </title>
</head>
<body>
<script>
var i= 0;
while(i<3)
{
document.write("<strong>outer loop</strong>");
 document.write(i);
 i++;
 document.write("<br>");
 var j=0;
 while(j<5)
 {
 document.write("inner loop");
 document.write(j);
 j++;
 document.write("<br>");
 }
 }
 </script>

</body>
</html>
o/p:
outer loop0
inner loop0
inner loop1
inner loop2
inner loop3
inner loop4
outer loop1
inner loop0
inner loop1
inner loop2
inner loop3
inner loop4
outer loop2
inner loop0
inner loop1
inner loop2
inner loop3
inner loop4

//Do  while Loop

<DOCTYPE html>
<html>
<head><title> monika bhosale </title>
</head>
<body>
<script>
var i= 0;
do
{
document.write("<strong>outer loop</strong>");
 document.write(i);
 i++;
 document.write("<br>");
 }
 while(i<5)
 </script>

</body>
</html>
