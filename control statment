// while loop

<html>
<head><title> monika bhosale </title>
</head>
<body>
<script>
var i=0;
while(i<5)
{
	document.write(i);
	i++;
	document.write("<br>");
	}
  document.write("rest of code");
	</script>

</body>
</html>
