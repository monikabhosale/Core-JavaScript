//Nested for loop
 for loop inside for loop.
 
 <html>
<head><title> monika bhosale </title>
</head>
<body>
<script>
for (i=0; i<3;i++)
{
   document.write("<strong>outer loop </strong>");
   document.write(i);
   document.write("<br>");
   for(j=0;j<5;j++)
{
	document.write("Inner loop");
	document.write("j");
	document.write("<br>");
 }
}
	</script>

</body>
</html>

