//
<!DOCTYPE html>
<html>
<head><title> arrow function with parameter</title>
</head>
<body>
<script>
  // arrow function with parameter
  var myfun = (a) =>{
  document.write(a);
  };
  myfun(10);
		   

	 
 </script>

</body>
</html>

o/p:
10
//
<!DOCTYPE html>
<html>
<head><title> arrow function with parameter</title>
</head>
<body>
<script>
  // arrow function with parameter
  var myfun = (a,b) =>{
  document.write(a + " " + b);
  };
  myfun(10,20);
		   

	 
 </script>

</body>
</html>


o/p; 1020
//
<!DOCTYPE html>
<html>
<head><title> arrow function with rest parameter</title>
</head>
<body>
<script>
  // arrow function with parameter
  var myfun = (a,...args) => {
  document.write(a + " " + args);
  };
  myfun(10,80,2,3,4,78,56,);
		   

	 
 </script>

</body>
</html>
o/p:
10 80,2,3,4,78,56

